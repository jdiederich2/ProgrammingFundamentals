Begin
  //Pseudocode goes here

  initialize number1 to 0
  initialize number2 to 0
  initialize number3 to 0
  
  prompt for number1 "Enter value for number1"
  prompt for number2 "Enter value for number2"
  prompt for number3 "Enter value for number3"
  
  IF number1 < number2 && number2 < number3 THEN 
    display number1 +  number2 + number3
    
  ELSEIF number2 < number1 && number1 < number3 THEN 
    display number2 +  number1 + number3 

  ELSEIF number3 < number2 && number2 < number1 THEN 
    display number3 +  number2 + number1 

  ELSEIF number1 < number3 && number3 < number2 THEN 
    display number1 +  number3 + number2
  
  ELSEIF number2 < number3 && number3 < number1 THEN 
      display number2 +  number3 + number1
  
  ELSEIF number3 < number1 && number1 < number2 THEN 
      display number3 +  number1 + number2
    
  EndIF
End


//Pseudocode to output all numbers from 1 to 100.

Begin
  initialize count to 1
  
  FOREACH number 1 to 100 increase in increments of 1
    count = count + 1
    
    display count
  
End



